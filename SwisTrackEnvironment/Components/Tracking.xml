<?xml version="1.0"?>
<swistrackcomponent>
	<friendlyname>Original Tracking Core (N. Correll)</friendlyname>
	<description>To be implemented</description>
	<url>http://en.wikibooks.org/wiki/SwisTrack/Components/Tracking</url>
	<displayimage>Output</displayimage>
	<configuration>
	    <parameter type="integer">
		    <name>MaxNumber</name>
			<label>Max. Number</label>
			<unit>Individuals</unit>
			<default>10</default>
		</parameter>
		<parameter type="double">
			<name>DistanceGate</name>
			<label>Distance Gate</label>
			<unit>pixels</unit>
			<slider>true</slider>
			<default>100</default>
		</parameter>
		<parameter type="checkbox">
			<name>ShareTrajectories</name>
			<label>Share Trajectories</label>
			<default>true</default>
		</parameter>

	</configuration>
</swistrackcomponent>

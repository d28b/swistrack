<?xml version="1.0"?>
<swistrackcomponent>
	<friendlyname>Output to MPEG4 File</friendlyname>
	<description>Write an MPEG4 video file.</description>
	<url>http://en.wikibooks.org/wiki/SwisTrack/Components/OutputFileM4V</url>
	<displayimage>Output</displayimage>

	<configuration>
		<parameter type="file">
			<name>Filename</name>
			<label>M4V File</label>
			<reloadable>false</reloadable>
			<isinput>false</isinput>
		</parameter>
		<parameter type="integer">
			<name>FrameRate</name>
			<label>Frame Rate</label>
			<default>15</default>
			<reloadable>false</reloadable>
		</parameter>
		<parameter type="dropdownlist">
			<name>InputImage</name>
			<label>Input Image</label>
			<items>
				<item key="0">Gray</item>
				<item key="1">Color</item>
				<item key="2">Binary</item>
			</items>
			<default>0</default>
			<reloadable>true</reloadable>
		</parameter>
	</configuration>
</swistrackcomponent>

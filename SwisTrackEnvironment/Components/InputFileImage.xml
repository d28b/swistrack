<?xml version="1.0"?>
<swistrackcomponent>
	<friendlyname>Input Static Image from a File</friendlyname>
	<description>Reads an image from a file.</description>
	<url>http://en.wikibooks.org/wiki/SwisTrack/Components/InputFileImage</url>
	<displayimage>Output</displayimage>

	<configuration>
		<parameter type="inputfile">
			<name>File</name>
			<label>Image file</label>
			<filetype>image</filetype>
			<onchange>restart</onchange>
		</parameter>
	</configuration>
</swistrackcomponent>

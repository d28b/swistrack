<?xml version="1.0"?>
<swistrackcomponent>
	<friendlyname>Binary mask</friendlyname>
	<description>Adds a mask on the binary image. This can be useful to ignore areas which could otherwise be detected as particles (e.g. borders).</description>
	<url>http://en.wikibooks.org/wiki/SwisTrack/Components/BinaryMask</url>
	<displayimage>Output</displayimage>
	<configuration>
		<parameter type="image">
			<name>MaskImage</name>
			<label>Mask image</label>
			<reloadable>false</reloadable>
		</parameter>
	</configuration>
</swistrackcomponent>

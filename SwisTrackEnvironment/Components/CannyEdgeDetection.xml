<?xml version="1.0"?>
<swistrackcomponent>
	<friendlyname>Canny Edge Detection</friendlyname>
	<description>Outputs the canny edges in the binary image.</description>
	<url>http://en.wikibooks.org/wiki/SwisTrack/Components/CannyEdgeDetection</url>
	<displayimage>Output</displayimage>

	<configuration>
	  <parameter type="double">
	    <name>Threshold1</name>
	    <label>Threshold 1</label>
	    <unit></unit>
	    <default>50</default>
	    <onchange>reload</onchange>
	  </parameter>
	  <parameter type="double">
	    <name>Threshold2</name>
	    <label>Threshold 2</label>
	    <unit></unit>
	    <default>200</default>
	    <onchange>reload</onchange>
	  </parameter>
	</configuration>
</swistrackcomponent>

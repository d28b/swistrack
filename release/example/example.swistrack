      <parameter name="BackgroundImage" value="bg.bmp"/>
      <parameter name="CorrectMean" value=""/>
    </component>
    <component type="ThresholdColorCommon">
      <parameter name="AverageBool" value="true"/>
      <parameter name="InvertThreshold" value=""/>
      <parameter name="Threshold" value="54"/>
    </component>
    <component type="BlobDetectionMinMax">
      <parameter name="MaxArea" value=""/>
      <parameter name="MaxNumber" value=""/>

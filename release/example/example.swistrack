    </component>
    <component type="Tracking">
      <parameter name="DistanceGate" value=""/>
      <parameter name="MaxNumber" value="10"/>
      <parameter name="ShareTrajectories" value="true"/>
    </component>
  </components>
  <server>
    <port>3000</port>
  </server>
</swistrack>

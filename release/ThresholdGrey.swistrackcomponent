<swistrackcomponent>
	<name>Threshold a greyscale image</name>
	<description>Thresholds a greyscale image.</description>
	<display>Output</display>

	<configuration>
		<parameter type="integer">
			<label>Threshold</label>
			<min>0</min>
			<max>255</max>
			<display>Output</display>
		</parameter>
	</configuration>
</swistrackcomponent>

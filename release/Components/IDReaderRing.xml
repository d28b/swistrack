<?xml version="1.0"?>
<swistrackcomponent>
	<friendlyname>ID Reader (Ring)</friendlyname>
	<description>Reads the ID of the robot from a ring placed around the blob. In addition, the orientation of the robot is detected.</description>
	<url>http://en.wikibooks.org/wiki/SwisTrack/ComponentList/IDReaderRing</url>
	<displayimage>Output</displayimage>

	<configuration>
		<parameter type="double">
			<name>RingRadiusOuter</name>
			<label>Max. blobs</label>
			<min>1</min>
			<max>50</max>
			<default>12</default>
			<unit>pixels</unit>
			<slider>true</slider>
		</parameter>
		<parameter type="double">
			<name>RingRadiusInner</name>
			<label>Inner radius</label>
			<min>1</min>
			<max>50</max>
			<default>8</default>
			<unit>pixels</unit>
		</parameter>
		<parameter type="file">
			<name>ObjectListFileName</name>
			<label>Objects and codes</label>
			<default></default>
		</parameter>
	</configuration>
</swistrackcomponent>

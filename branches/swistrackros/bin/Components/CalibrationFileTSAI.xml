<?xml version="1.0"?>
<swistrackcomponent>
	<friendlyname>Calibration with TSAI (File)</friendlyname>
	<description>Use the TSAI model to calibrate the camera. The model parameters must be computed outside of SwisTrack. When calibration points are provided, the calibration errors are computed.</description>
	<url>http://en.wikibooks.org/wiki/SwisTrack/Components/CalibrationFileTSAI</url>
	<displayimage>Output</displayimage>

	<configuration>
		<parameter type="inputfile">
			<name>CalibrationPoints</name>
			<label>Calib. Points</label>
		</parameter>
		<parameter type="inputfile">
			<name>CalibrationParameters</name>
			<label>Calib. Param.</label>
		</parameter>
	</configuration>
</swistrackcomponent>
